{
  "name": "@viewdo/ui",
  "version": "0.1.0",
  "homepage": "https://github.com/viewdo/ui#readme",
  "bugs": {
    "url" : "https://github.com/viewdo/ui/issues",
    "email" : "project@hostname.com"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/viewdo/ui.git"
  },
  "license": "MIT",
  "distDirs": {
    "stencil": "dist",
    "storybook": "www/storybook",
    "docs": "docs"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "loader": "dist/loader",
  "collection": "dist/collection/collection-manifest.json",
  "scripts": {
    "npm:install": "npm install",
    "--- development ---": "---------------",
    "start": "concurrently -n storybook,stencil,proxy -c \"bgMagenta.bold,bgBlue.bold,bgCyan.bold\" \"npm:watch:storybook\" \"npm:watch:stencil\" \"npm:watch:proxy\"",
    "watch:storybook": "start-storybook -p 3001 --ci --quiet --output-dir ./www/storybook --static-dir ./www/build/experience-ui",
    "watch:stencil": "stencil build --dev --watch --serve --port 3002",
    "watch:proxy": "wait-on http://localhost:3001/ && wait-on http://localhost:3002/ && node ./.storybook/proxy.js",
    "gen": "stencil generate",
    "--- production ---": "---------------",
    "build": "run-s build:stencil build:storybook",
    "build:stencil": "stencil build --docs",
    "build:storybook": "build-storybook --output-dir ./www/storybook --static-dir ./www",
    "build:storybook:docs": "build-storybook --docs --output-dir ./www/storybook --static-dir ./www",
    "--- test ---": "---------------",
    "test": "stencil test --spec --coverage",
    "test:spec": "stencil test --spec",
    "test:e2e": "stencil test --e2e",
    "test:watch": "stencil test --spec --watch",
    "--- lint ---": "---------------",
    "lint": "run-p lint:js lint:css",
    "lint:js": "eslint src/**/*{.ts,.tsx}",
    "lint:css": "stylelint 'src/**/*.scss' --allow-empty-input",
    "lint:husky": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:spec"
    }
  },
  "dependencies": {
    "@ionic/core": "^5.5.0",
    "expr-eval": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@stencil/core": "^2.3.0",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/store": "^1.3.0",
    "@storybook/addon-actions": "^6.1.6",
    "@storybook/addon-essentials": "^6.1.6",
    "@storybook/addon-links": "^6.1.6",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/web-components": "^6.1.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/puppeteer": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.1.1",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^10.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "http-proxy-middleware": "^1.0.5",
    "husky": "^4.3.0",
    "install-peers": "^1.0.3",
    "jest": "^26.4.2",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.4.2",
    "lit-html": "^1.3.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "puppeteer": "^5.3.1",
    "stylelint": "^13.7.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "typescript": "^4.0.3",
    "write-file-webpack-plugin": "^4.5.1",
    "yargs": "^16.1.1"
  },
  "jest": {
    "preset": "@stencil/core/testing"
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
